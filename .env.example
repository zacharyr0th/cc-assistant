# Preset: quality-focused, security-focused, react-focused, all, off
# See: /enable-hook <preset>
HOOKS_PRESET=off

# Individual hook toggles (if not using preset)
HOOK_CHECK_AFTER_EDIT=false
HOOK_SECURITY_SCAN=false
HOOK_CODE_QUALITY=false
